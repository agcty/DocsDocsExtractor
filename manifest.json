{
  "name": "DocsDocs Extractor",
  "version": "0.0.1",
  "manifest_version": 2,
  "background": {
    "scripts": ["background.js"],
    "persistent": false
  },
  "browser_action": {
    "default_icon": "icons/logo.png"
  },
  "permissions": ["activeTab", "<all_urls>", "storage", "tabs"],
  "web_accessible_resources": ["result.html"]
}
